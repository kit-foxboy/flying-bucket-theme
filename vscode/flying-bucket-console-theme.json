  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#bec9c699",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#adcae6"
      }
    },
    {
      "name": "Escape Character",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#ffb4ab"
      }
    },
    {
      "name": "Null",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#ffb4abcc"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#82d5c9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#dde4e1"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#82d5c9dd"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#9ef2e5"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#adcae6"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#dde4e1"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#bec9c6"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#82d5c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#82d5c9"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#adcae6"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#82d5c9"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#adcae6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#82d5c9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#dde4e1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#b1ccc7"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#82d5c9"
      }
    }
  ]
}
